{
    "name": "aurora-app",
    "version": "0.0.0",
    "description": "",
    "author": "José Carlos Rodríguez Palacín <carlos.rodriguez.palacin@gmail.com>",
    "private": true,
    "license": "MIT",
    "scripts": {
        "install:core": "rimraf node_modules/@aurorajs.dev/core && npm i @aurorajs.dev/core --install-links",
        "install:typesense": "rimraf node_modules/@aurorajs.dev/typesense && npm i @aurorajs.dev/typesense --install-links",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:repl": "nest start --entryFile repl",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "graphql:types": "ts-node generate-typings",
        "prepare": "husky",
        "commit": "cz"
    },
    "dependencies": {
        "@apollo/server": "^4.11.3",
        "@aurorajs.dev/core": "file:../aurora-core",
        "@aurorajs.dev/typesense": "file:../aurora-typesense",
        "@azure/storage-blob": "^12.27.0",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@modelcontextprotocol/sdk": "^1.17.4",
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/apollo": "^13.0.3",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bullmq": "^11.0.2",
        "@nestjs/cache-manager": "^3.0.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.1",
        "@nestjs/core": "^11.0.1",
        "@nestjs/cqrs": "^11.0.2",
        "@nestjs/graphql": "^13.0.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.0.1",
        "@nestjs/schedule": "^5.0.1",
        "@nestjs/serve-static": "^5.0.3",
        "@nestjs/swagger": "^11.0.6",
        "@sentry/node": "^9.5.0",
        "bullmq": "^5.42.0",
        "cache-manager": "^6.4.1",
        "chalk": "^4.1.2",
        "graphql": "^16.10.0",
        "graphql-type-json": "^0.3.2",
        "graphql-upload": "^17.0.0",
        "handlebars-helpers": "^0.10.0",
        "jwks-rsa": "^3.2.0",
        "libphonenumber-js": "^1.12.5",
        "nest-winston": "^1.10.2",
        "nestjs-i18n": "^10.5.1",
        "openai": "^4.98.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.14.0",
        "pg-hstore": "^2.3.4",
        "redis": "^4.7.0",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^6.0.1",
        "rxjs": "^7.8.1",
        "semver": "^7.7.2",
        "sequelize": "^6.37.6",
        "sharp": "^0.33.5",
        "ts-morph": "^25.0.1",
        "winston": "^3.17.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/js": "^9.22.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@stylistic/eslint-plugin-ts": "^4.2.0",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.7",
        "@types/supertest": "^6.0.2",
        "commitizen": "^4.3.1",
        "eslint": "^9.22.0",
        "fs-extra": "^11.3.0",
        "globals": "^16.0.0",
        "gulp": "^5.0.0",
        "gulp-json-editor": "^2.6.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "@aurora/(.*)": "<rootDir>/../src/@aurora/$1",
            "@app/(.*)": "<rootDir>/../src/@app/$1",
            "@api/(.*)": "<rootDir>/../src/@api/$1"
        }
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
